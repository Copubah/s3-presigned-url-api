# AWS Configuration
aws_region = "us-east-1"
environment = "dev"
project_name = "s3-presigned-api"

# S3 Configuration
bucket_name = "my-s3-presigned-api-bucket-dev"
cors_allowed_origins = ["http://localhost:3000", "https://myapp.com"]
file_retention_days = 90

# Logging
log_retention_days = 30

# ECS Deployment (optional)
deploy_to_ecs = false
vpc_id = ""
public_subnet_ids = []
private_subnet_ids = []